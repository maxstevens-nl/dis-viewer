{
  "name": "zapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev:ui": "vite",
    "dev:zero-cache": "zero-cache-dev",
    "dev:db-up": "docker compose --env-file .env -f ./docker/docker-compose.yml up",
    "dev:db-down": "docker compose --env-file .env -f ./docker/docker-compose.yml down",
    "dev:clean": "source .env && docker volume rm -f docker_dis_viewer_pgdata && rm -rf \"${ZERO_REPLICA_FILE}\"*",
    "seed": "bun run scripts/seed.ts",
    "schema:generate": "drizzle-zero generate --schema ./api/drizzleSchema.ts --output ./src/schema.ts",
    "build": "tsc -b && vite build",
    "lint": "eslint ."
  },
  "dependencies": {
    "@rocicorp/zero": "^0.25.2",
    "@types/papaparse": "^5.5.2",
    "drizzle-orm": "^0.45.1",
    "papaparse": "^5.4.1",
    "pg-copy-streams": "^7.0.0",
    "pg": "^8.16.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "sst": "3.9.33",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@eslint/js": "^9.9.0",
    "@hono/node-server": "^1.13.2",
    "@types/pg-copy-streams": "^1.2.2",
    "@types/bun": "^1.1.20",
    "@types/node": "^22.7.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "dotenv": "^16.4.5",
    "drizzle-zero": "^0.17.3",
    "drizzle-kit": "^0.31.9",
    "eslint": "^9.9.0",
    "eslint-plugin-react-hooks": "^5.1.0-rc.0",
    "eslint-plugin-react-refresh": "^0.4.9",
    "globals": "^15.9.0",
    "hono": "^4.6.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.0.1",
    "vercel": "^41.3.2",
    "vite": "^5.4.1"
  },
  "trustedDependencies": [
    "@rocicorp/zero-sqlite3"
  ],
  "pnpm": {
    "onlyBuiltDependencies": [
      "@rocicorp/zero-sqlite3"
    ]
  },
  "_": "For some reason, sst requires esbuild 0.25.0 when deploying the permission deployer",
  "overrides": {
    "esbuild": "0.25.0"
  }
}
